<Window x:Class="InternationalWagesManager.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InternationalWagesManager.Views"
        mc:Ignorable="d"
        ResizeMode="CanResizeWithGrip"
        Title="Main Window"
        >
    <Grid>
        <Frame Name="MainWindowFrame" 
               Loaded="MainWindowFrame_Loaded"
               NavigationUIVisibility="Hidden" ClipToBounds="True"></Frame>
               <!--Width="{Binding RelativeSource={RelativeSource AncestorType=Window}, Path=ActualWidth}"
               Height="{Binding RelativeSource={RelativeSource AncestorType=Window}, Path=ActualHeight}"-->
    </Grid>
</Window>
